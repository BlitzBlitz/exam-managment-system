<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.StudentResultsController">
   <children>
      <VBox prefHeight="350.0" prefWidth="300.0" style="-fx-background-color: #274690;">
         <children>
            <Label alignment="CENTER" prefHeight="50.0" prefWidth="300.0" text="Your Results" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <ScrollPane hbarPolicy="NEVER" prefHeight="267.0" prefWidth="280.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <content>
                  <VBox fx:id="resultsContainer" prefHeight="264.0" prefWidth="275.0">
                     </VBox>
               </content>
            </ScrollPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
